{
  "name": "wrd-watch-gcc",
  "version": "2.0.0",
  "description": "Target description for the ARM mbed Wearable Reference Design Watch using GCC.",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "inherits": {
    "mbed-gcc": "^1.1.0"
  },
  "keywords": [
    "mbed-target:wrd-watch",
    "mbed-official",
    "efm32gg",
    "watch",
    "gcc"
  ],
  "similarTo": [
    "max1704",
    "framebuffer-1bit",
    "matrixlcd-sharp-1bit",
    "onsemi-ncp5623c",
    "nxp-pcal64",
    "watch",
    "efm32gg-stk",
    "efm32gg",
    "efm32",
    "silabs",
    "cortex-m3",
    "armv7-m",
    "arm",
    "gcc",
    "swo"
  ],
  "config": {
    "cmsis": {
      "nvic": {
        "ram_vector_address": "(&__start_vector_table__)",
        "flash_vector_address": "0x0",
        "user_irq_offset": 16,
        "user_irq_number": 39
      }
    },
    "uvisor": {
      "present": false
    },
    "mbed-os": {
      "stdio": {
        "default-baud": 115200
      }
    },
    "minar": {
      "stats": true
    },
    "hardware": {
      "clock": {
        "core": {
          "source": "HFXO",
          "frequency": 48000000
        },
        "low-energy": {
          "source": "LFXO",
          "frequency": 32768
        }
      },
      "pins": {
        "LED0": "NC",
        "LED1": "NC",
        "LED2": "NC",
        "LED3": "NC",
        "LED4": "NC",
        "USBTX": "PB3",
        "USBRX": "PB4",
        "UART_TX": "USBTX",
        "UART_RX": "USBRX",
        "SERIAL_TX": "USBTX",
        "SERIAL_RX": "USBRX",
        "STDIO_UART_TX": "USBTX",
        "STDIO_UART_RX": "USBRX",
        "EFM_BC_EN": "NC"
      },
      "test-pins": {
        "spi": {
          "MOSI": "NC",
          "MISO": "NC",
          "SCLK": "NC",
          "SSEL": "NC"
        },
        "i2c": {
          "SDA": "NC",
          "SCL": "NC"
        }
      },
      "lesense": {
        "port": "gpioPortC",
        "total-channels": 16
      },
      "wearable-reference-design": {
        "battery": {
          "present": true,
          "capacity": 140000,
          "estimated-current": 100
        },
        "battery-backup": {
          "present": true
        },
        "battery-gauge": {
          "present": true,
          "i2c": {
            "name": "i2c0"
          },
          "pin": {
            "irq": "NC"
          }
        },
        "bit-band": {
          "present": true,
          "ram-base": "0x20000000UL",
          "bit-band-base": "0x22000000UL"
        },
        "bluetooth-le": {
          "present": true,
          "spi": {
            "name": "spi0",
            "cs": "PC14"
          },
          "pin": {
            "nrdy": "PD10",
            "nreq": "PD11"
          }
        },
        "button": {
          "present": true,
          "pin": {
            "forward": "PC15"
          }
        },
        "buzzer": {
          "present": true,
          "pin": {
            "enable": "PA8"
          }
        },
        "external-gpio": {
          "present": true,
          "i2c": {
            "name": "i2c0"
          },
          "pin": {
            "irq1": "PB6",
            "irq2": "PF3"
          }
        },
        "external-sram": {
          "present": true,
          "pin": {
            "cs": "PD9",
            "write-enable": "PF4",
            "output-enable": "PF5"
          }
        },
        "gps": {
          "present": true,
          "spi": {
            "mosi": "PC11",
            "miso": "PC10",
            "clk": "PC9",
            "cs": "PD7"
          },
          "uart": {
            "tx": "PF6",
            "rx": "PF7"
          },
          "pin": {
            "timepulse": "PA9"
          }
        },
        "interface": {
          "i2c": {
            "size": 1,
            "i2c0": {
              "name": "i2c0",
              "sda": "PD14",
              "scl": "PD15"
            }
          },
          "spi": {
            "size": 3,
            "spi0": {
              "name": "spi0",
              "mosi": "PC11",
              "miso": "PC10",
              "sclk": "PC9"
            },
            "spi1": {
              "name": "spi1",
              "mosi": "PD0",
              "miso": "NC",
              "sclk": "PD2"
            },
            "spi2": {
              "name": "spi2",
              "mosi": "PB3",
              "miso": "PB4",
              "sclk": "PB5"
            }
          }
        },
        "micro-sd": {
          "present": true,
          "pin": {
            "cs": "PF9"
          }
        },
        "nfc": {
          "present": true,
          "pin": {
            "irq": "PA7"
          }
        },
        "output-compare": {
          "present": true
        },
        "rfid": {
          "present": true,
          "pin": {
            "modulator": "PB11",
            "envelope": "PC5",
            "clock": "PF8"
          }
        },
        "rgb-led": {
          "present": false,
          "i2c": {
            "name": "i2c0"
          }
        },
        "screen": {
          "present": true,
          "spi": {
            "name": "spi1",
            "cs": "PA11"
          },
          "pin": {
            "enable": "PD1",
            "refresh": "PB12"
          }
        },
        "sensor-accelerometer": {
          "present": true,
          "i2c": {
            "name": "i2c0"
          },
          "enable": {
            "location": "0x42",
            "pin": 5
          },
          "irq": {
            "location": "0x42",
            "pin": 9
          }
        },
        "sensor-gyroscope": {
          "present": true,
          "i2c": {
            "name": "i2c0"
          },
          "enable": {
            "location": "0x42",
            "pin": 5
          },
          "irq": {
            "location": "0x42",
            "pin": 12
          }
        },
        "sensor-battery-current": {
          "present": true,
          "pin": {
            "adc": "PD4"
          }
        },
        "sensor-battery-charging-current": {
          "present": true,
          "pin": {
            "adc": "PD6"
          }
        },
        "sensor-battery-voltage": {
          "present": true,
          "pin": {
            "adc": "PD5"
          }
        },
        "sensor-fingerprint": {
          "present": true,
          "spi": {
            "mosi": "PB3",
            "miso": "PB4",
            "clk": "PB5",
            "cs": "PF12"
          },
          "pin": {
            "nrdy": "PD12",
            "nreq": "PD13"
          }
        },
        "sensor-temperature": {
          "present": true,
          "pin": {
            "enable": "PB15",
            "adc": "PD3"
          }
        },
        "swo": {
          "present": true,
          "enabled": false
        },
        "touch": {
          "present": true,
          "slider-1": 1,
          "slider-2": 3,
          "slider-3": 0,
          "slider-4": 2,
          "slider-top": 12,
          "slider-back": 4,
          "slider-bottom": 13,
          "channels-in-use": 5,
          "sensitivity": 97,
          "sample-delay": 90,
          "calibration-values": 10,
          "scan-frequency-low": 20,
          "scan-frequency-high": 20
        },
        "vibration-motor": {
          "present": true,
          "pulse-width-unit-us": 200,
          "pin": {
            "enable": "PA10"
          }
        },
        "watchdog": {
          "present": true,
          "enabled": true
        }
      }
    },
    "modules": {
      "analogin": {
        "size": 1
      },
      "analogout": {
        "size": 1
      },
      "gpio": {
        "size": 1
      },
      "i2c": {
        "size": 2
      },
      "pwmout": {
        "size": 1
      },
      "serial": {
        "size": 7,
        "stdio-uart": "UART0"
      },
      "spi": {
        "size": 3,
        "transaction-queue-size": 4
      }
    }
  },
  "toolchain": "CMake/toolchain.cmake"
}
